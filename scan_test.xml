<config>
    <execution_count>2</execution_count>
    <global_env>
        <own_ip>None</own_ip>
        <dst_ip>192.168.1.3</dst_ip>
        <ping_param>-d 5</ping_param>
        <savelog>yes</savelog>
    </global_env>
    <device>
        <name>/dev/ttyUSB0</name>
        <baudrate>115200</baudrate>
        <parity>N</parity>
        <data_bits>8</data_bits>
        <stop_bits>1</stop_bits>
    </device>
    <extra_device>
        <name>/dev/ttyUSB1</name>
        <baudrate>115200</baudrate>
        <parity>N</parity>
        <data_bits>8</data_bits>
        <stop_bits>1</stop_bits>
    </extra_device>
    <commands>
        <command execute="yes">
            <text>rtos_info</text>
            <repeat>1</repeat>
            <interval>1</interval>
            <recv_interval>1</recv_interval>
            <prerun>yes</prerun>
        </command>
        <command execute="yes">
            <text>extlog</text>
            <cmd_attri>EXTLOG</cmd_attri>
            <repeat>1</repeat>
            <interval>1</interval>
            <recv_interval>1</recv_interval>
            <prerun>yes</prerun>
        </command>
        <command execute="yes">
            <text>wifi_scan</text>
            <repeat>1</repeat>
            <interval>5</interval>
            <recv_interval>1</recv_interval>
        </command>
        <command execute="yes">
            <text>wifi_connect -s HUAWEI_AX6 -k 12345678</text>
            <repeat>1</repeat>
            <interval>8</interval>
            <recv_interval>1</recv_interval>
            <parse_rslt>IP</parse_rslt>
        </command>
        <!--command execute="yes">
            <text>ping 192.168.1.3 -d 5</text>
            <cmd_attri>DSTIP</cmd_attri>
            <repeat>1</repeat>
            <interval>5</interval>
            <recv_interval>1</recv_interval>
        </command-->
        <command execute="yes">
            <text>ping</text>
            <cmd_attri>DST_IP PING_PARAM</cmd_attri>
            <repeat>1</repeat>
            <interval>5</interval>
            <recv_interval>1</recv_interval>
        </command>
        <command execute="yes">
            <text>wifi_disconnect</text>
            <repeat>1</repeat>
            <interval>1.0</interval>
            <recv_interval>1</recv_interval>
        </command>
        <command execute="no">
            <text>lsb_release -a</text>
            <native>yes</native>
            <repeat>3</repeat>
            <interval>1.0</interval>
            <recv_interval>1</recv_interval>
        </command>
    </commands>
</config>

